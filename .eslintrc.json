{
  "extends": "airbnb",
  "rules": {
    "no-undef": 0,
    "no-param-reassign": 0,
    "eqeqeq": 0,
    "max-len": 0,
    "no-use-before-define": 0,
    "space-before-function-paren": 0,
    "no-restricted-syntax": ["off", "ForInStatement"],
    "arrow-body-style": 0,
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  }
}
